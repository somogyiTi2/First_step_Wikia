<%-include("partials/header")-%>
<h1>Home</h1>
<% if (first.length>100){%>
    <p> <%=first.substring(0, 99)+"..."%></p>
  <%}else { %>
    <p><%=first%></p>
  <%}%>

<!--AZS megoldása:-->
<% posztocskak.forEach(function(posztocskak){%>
<h1><%=posztocskak.post%></h1>
<% if (posztocskak.cik.length>100){%>
    <p>
       <%=posztocskak.cik.substring(0, 99)+"..."%>
         <a href='/posts/<%=posztocskak.post%>'>Bővebben...</a>
    </p>

  <%}else { %>
    <p><%=posztocskak.cik%></p>
  <%}%>
<%});%>


